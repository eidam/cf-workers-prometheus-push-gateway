{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "noEmit": true,
    "lib": [
      "esnext"
    ],
    "jsx": "react",
    "resolveJsonModule": true,
    "types": [
      "@cloudflare/workers-types"
    ]
  }
}
